<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>validation_rule_for_email_and_phone</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK( Email__c ) , ISBLANK( Phone__c ) )</errorConditionFormula>
    <errorMessage>You must fill email and phone before 
saving</errorMessage>
</ValidationRule>
